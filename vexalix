-- HĂ m Ä‘á»•i mĂ u rainbow
local function rainbowText()
    local hue = 0
    while task.wait(0.1) do -- Äá»•i mĂ u má»—i 0.1 giĂ¢y
        hue = (hue + 10) % 360 -- TÄƒng hue Ä‘á»ƒ táº¡o hiá»‡u á»©ng chuyá»ƒn mĂ u
        textLabel.TextColor3 = Color3.fromHSV(hue / 360, 1, 1) -- Chuyá»ƒn mĂ u
    end
end

-- Cháº¡y hiá»‡u á»©ng rainbow
task.spawn(rainbowText)

